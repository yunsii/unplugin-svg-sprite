{
  "name": "unplugin-svg-sprite",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.0",
  "description": "Unified plugin system for svg sprite",
  "license": "MIT",
  "homepage": "https://github.com/yunsii/unplugin-svg-sprite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yunsii/unplugin-svg-sprite.git"
  },
  "bugs": {
    "url": "https://github.com/yunsii/unplugin-svg-sprite/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "play": "npm -C playground/vite-react run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint-staged:lint": "eslint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --check --write --no-plugin-search \"**/*.(js|jsx|tsx|ts|css|less|scss|sass|md|yaml)\""
  },
  "dependencies": {
    "consola": "^3.2.3",
    "dedent": "^1.2.0",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "pathe": "^1.1.1",
    "svg-sprite": "^2.0.2",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/eslint-config-react": "^0.35.3",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/types": "^17.4.4",
    "@jannajs/lint": "^2.3.5",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.16",
    "@types/svg-sprite": "^0.0.34",
    "bumpp": "^9.1.0",
    "chalk": "^5.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.6.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.22",
    "prettier": ">=2.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vinyl": "^3.0.0",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "webpack": "^5.85.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:lint",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,yaml}": [
      "prettier --write --no-plugin-search"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
